<?xml version="1.0" encoding="utf-8"?>
<PreferenceScreen xmlns:android="http://schemas.android.com/apk/res/android">
    <EditTextPreference
        android:key="@string/pref_location_key"
        android:title="@string/pref_location"
        android:defaultValue="@string/default_pref_location"
        android:inputType="text"
        android:singleLine="true" />

    <ListPreference
        android:key="@string/pref_type_key"
        android:title="@string/pref_type"
        android:defaultValue="@string/default_pref_type"
        android:entries="@array/pref_type_entries"
        android:entryValues="@array/pref_type_values"
       />

</PreferenceScreen>
